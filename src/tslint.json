{
    "extends": "tslint:recommended",
    "rules": {
        /*"no-unused-variable": [
            true,
            "react"
        ],*/
        "object-literal-sort-keys": false,
        "ordered-imports": [
            false
        ],
        "no-string-literal": false,
        "no-unused-new": false,
        "whitespace": [
            false
        ],
        "trailing-comma": [
            false
        ],
        "max-classes-per-file": [
            false
        ],
        "member-ordering": [
            false
        ],
        "curly": false,
        "one-line": [
            false
        ],
        "max-line-length": [
            255
        ],
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore"
        ],
        "one-variable-per-declaration": [
            false
        ],
        "prefer-for-of": false,
        "radix": false,
        "no-namespace": [
            false
        ],
        "no-console": [
            false
        ],
        "quotemark": [
            false,
            "double",
            "avoid-escape"
        ],
        "comment-format": [
            false
        ]
    }
}